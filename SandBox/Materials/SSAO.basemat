Basemat:
  Pipelines:
    - RenderPass: SSAO
      Type: Compute
      Compute: Shaders/SSAO.comp
      DescriptorSets:
        - Type: Renderer
          RenderPass: SSAO
          Set: 0
        - Type: Renderer
          RenderPass: DefaultReflection
          Set: 1
      Uniforms:
        - Name: normalTexture
          RenderTarget: "GBuffer[1]"
        - Name: depthTexture
          RenderTarget: "GBuffer[4]"
