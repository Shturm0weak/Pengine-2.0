Mat:
  Basemat: Materials/MeshBase.basemat
  Pipelines:
    - RenderPass: GBuffer
      Uniforms:
        - Name: albedoTexture
          Value: ""
        - Name: normalTexture
          Value: ""
        - Name: metalnessTexture
          Value: ""
        - Name: roughnessTexture
          Value: ""
        - Name: aoTexture
          Value: ""
        - Name: material
          Values:
            - Name: color
              Type: vec4
              Value: [1, 1, 1, 1]
            - Name: metallicFactor
              Type: float
              Value: 0.0
            - Name: roughnessFactor
              Type: float
              Value: 1.0
            - Name: aoFactor
              Type: float
              Value: 1.0
            - Name: useNormalMap
              Type: int
              Value: 0