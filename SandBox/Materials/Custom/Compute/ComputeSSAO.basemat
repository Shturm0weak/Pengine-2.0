Basemat:
  Pipelines:
    - RenderPass: ComputeSSAO
      Type: Compute
      Compute: Shaders/Custom/ComputeSSAO.comp
      DescriptorSets:
        - Type: Renderer
          RenderPass: ComputeSSAO
          Set: 0
        - Type: Renderer
          RenderPass: DefaultReflection
          Set: 1
      Uniforms:
        - Name: normalTexture
          RenderTarget: "GBuffer[1]"
        - Name: depthTexture
          RenderTarget: "GBuffer[4]"
